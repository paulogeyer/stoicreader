<div class="row">
  <div class="col-md-3">
    <h4>Your Subscriptions</h4>
    <ul class="list-group">
      <% @subscriptions.each do |subscription| %>
      <li class="list-group-item">
	<%= link_to subscription.title, subscription %>
      </li>
      <% end %>
    </ul>
  </div>
  <div class="col-md-9">
    <h4>Search Feeds</h4>
    <ul class="list-group">
      <% @feeds.each do |feed| %>
      <li class="list-group-item">
	<%= feed.title %>
      </li>
      <% end %>
    </ul>
  </div>
</div>
